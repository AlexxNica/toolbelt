{
  "name": "vtex",
  "version": "0.1.0",
  "description": "CLI tool for creating and managing VTEX apps",
  "bin": {
    "vtex": "lib/vtex.js",
    "vtex-login": "lib/vtex-login.js",
    "vtex-logout": "lib/vtex-logout.js",
    "vtex-watch": "lib/vtex-watch.js",
    "vtex-publish": "lib/vtex-publish.js"
  },
  "scripts": {
    "start": "node lib/vtex.js",
    "watch": "coffee --output lib --compile --watch --bare --no-header src",
    "prepublish": "npm test && coffee --bare --no-header --output lib --compile src",
    "test": "mocha --compilers coffee:coffee-script/register",
    "test-watch": "mocha --compilers coffee:coffee-script/register --watch"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vtex/toolbelt.git"
  },
  "keywords": [
    "CLI",
    "releasy",
    "release",
    "semver"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vtex/toolbelt/issues"
  },
  "dependencies": {
    "commander": "~2.7.0",
    "prompt": "~0.2.14",
    "q": "~1.2.0",
    "request": "^2.53.0",
    "semver": "~4.3.1",
    "shelljs": "~0.3.0"
  },
  "preferGlobal": true,
  "devDependencies": {
    "coffee-script": "1.9.0",
    "chai": "^2.1.1",
    "mocha": "^2.2.1"
  }
}
