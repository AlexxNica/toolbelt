{
  "name": "vtex",
  "version": "0.8.6",
  "description": "CLI tool for creating and managing VTEX apps",
  "bin": {
    "vtex": "lib/vtex.js",
    "vtex-login": "lib/vtex-login.js",
    "vtex-logout": "lib/vtex-logout.js",
    "vtex-watch": "lib/vtex-watch.js",
    "vtex-publish": "lib/vtex-publish.js"
  },
  "scripts": {
    "start": "node lib/vtex.js",
    "watch": "grunt",
    "prepublish": "npm test && grunt build",
    "test": "mocha --compilers coffee:coffee-script/register",
    "test-watch": "mocha --compilers coffee:coffee-script/register --watch"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vtex/toolbelt.git"
  },
  "keywords": [
    "CLI",
    "vtex",
    "toolbelt",
    "apps"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vtex/toolbelt/issues"
  },
  "dependencies": {
    "archiver": "^0.14.3",
    "chalk": "^1.0.0",
    "chokidar": "^1.0.0-rc4",
    "docopt": "^0.6.2",
    "glob": "^5.0.3",
    "node-libs-browser": "^0.5.2",
    "prompt": "~0.2.14",
    "q": "~1.2.0",
    "request": "^2.53.0",
    "semver": "~4.3.1",
    "shelljs": "~0.3.0",
    "supports-color": "^2.0.0",
    "tiny-lr": "^0.1.5",
    "vtexsay": "^1.0.3",
    "webpack": "^1.10.1",
    "webpack-dev-server": "^1.10.1"
  },
  "preferGlobal": true,
  "devDependencies": {
    "chai": "^2.1.1",
    "coffee-script": "1.9.0",
    "grunt": "^0.4.5",
    "grunt-coffeelint": "0.0.13",
    "grunt-contrib-coffee": "~0.12.0",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-watch": "^0.6.1",
    "mocha": "^2.2.1"
  }
}
