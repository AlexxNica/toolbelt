{
  "name": "vtex",
  "version": "1.0.0-beta.1",
  "description": "The platform for e-commerce apps.",
  "main": "lib/index.js",
  "bin": "lib/cli.js",
  "scripts": {
    "watch": "babel src --out-dir=lib --watch",
    "lint": "eslint src",
    "test": "npm run lint && nyc ava src/*.test.js src/**/*.test.js",
    "test-watch": "ava src/*.test.js src/**/*.test.js --watch",
    "prepublish": "babel src --out-dir=lib --ignore=*.test.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vtex/toolbelt.git"
  },
  "keywords": [
    "CLI",
    "vtex",
    "ecommerce",
    "commerce",
    "omnichannel",
    "platform",
    "react",
    "toolbelt",
    "apps"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vtex/toolbelt/issues"
  },
  "dependencies": {
    "archiver": "^1.0.0",
    "bluebird": "^3.4.0",
    "chalk": "^1.1.3",
    "chokidar": "^1.2.0",
    "configstore": "^2.0.0",
    "es6-error": "^3.0.0",
    "express": "^4.13.3",
    "glob": "^7.0.0",
    "http-proxy": "^1.11.2",
    "http-status-name": "^2.0.0",
    "log-symbols": "^1.0.2",
    "minimist": "^1.2.0",
    "ora": "^0.2.3",
    "ramda": "^0.21.0",
    "request": "^2.72.0",
    "request-promise": "^3.0.0",
    "semver": "^5.0.3",
    "shelljs": "^0.7.0",
    "supports-color": "^3.1.1",
    "tiny-lr": "^0.2.0",
    "update-notifier": "^0.7.0",
    "webpack": "^1.12.10",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.4.1",
    "winston": "^2.2.0"
  },
  "preferGlobal": true,
  "devDependencies": {
    "ava": "^0.15.0",
    "babel-cli": "^6.1.18",
    "babel-core": "^6.1.21",
    "babel-eslint": "^6.0.2",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "babel-register": "^6.9.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.10.2",
    "eslint-config-vtex": "^3.0.1",
    "eslint-plugin-react": "^5.0.1",
    "mocha": "^2.3.3",
    "mock-fs": "^3.4.0",
    "nock": "^8.0.0",
    "nyc": "^6.4.4"
  },
  "ava": {
    "failFast": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
