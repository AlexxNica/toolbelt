{
  "name": "vtex",
  "version": "0.10.4",
  "description": "CLI tool for creating and managing VTEX apps",
  "bin": {
    "vtex": "lib/vtex.js",
    "vtex-login": "lib/vtex-login.js",
    "vtex-logout": "lib/vtex-logout.js",
    "vtex-watch": "lib/vtex-watch.js",
    "vtex-publish": "lib/vtex-publish.js"
  },
  "scripts": {
    "start": "node lib/vtex.js",
    "watch": "grunt",
    "prepublish": "npm test && grunt build",
    "test": "mocha --compilers coffee:coffee-script/register",
    "test-watch": "mocha --compilers coffee:coffee-script/register --watch"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vtex/toolbelt.git"
  },
  "keywords": [
    "CLI",
    "vtex",
    "toolbelt",
    "apps"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vtex/toolbelt/issues"
  },
  "dependencies": {
    "archiver": "^0.15.1",
    "chalk": "^1.1.1",
    "chokidar": "^1.2.0",
    "docopt": "^0.6.2",
    "express": "^4.13.3",
    "glob": "^5.0.15",
    "http-proxy": "^1.11.2",
    "node-libs-browser": "^0.5.3",
    "prompt": "~0.2.14",
    "q": "^1.4.1",
    "request": "^2.64.0",
    "semver": "^5.0.3",
    "shelljs": "^0.5.3",
    "supports-color": "^3.1.1",
    "tiny-lr": "^0.2.0",
    "vtexsay": "^1.0.3",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.4.1",
    "signalr-client": "0.0.16"
  },
  "preferGlobal": true,
  "devDependencies": {
    "chai": "^3.3.0",
    "coffee-script": "^1.10.0",
    "grunt": "^0.4.5",
    "grunt-contrib-coffee": "^0.13.0",
    "grunt-contrib-copy": "^0.8.1",
    "grunt-contrib-watch": "^0.6.1",
    "mocha": "^2.3.3"
  }
}
